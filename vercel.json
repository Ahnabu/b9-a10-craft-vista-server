{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/getEndpoint",
            "dest": "server.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/postEndpoint",
            "dest": "server.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/api/putEndpoint",
            "dest": "server.js",
            "methods": [
                "PUT"
            ]
        },
        {
            "src": "/api/deleteEndpoint",
            "dest": "server.js",
            "methods": [
                "DELETE"
            ]
        },
        {
            "src": "/api/patchEndpoint",
            "dest": "server.js",
            "methods": [
                "PATCH"
            ]
        },
        {
            "src": "/api/optionsEndpoint",
            "dest": "server.js",
            "methods": [
                "OPTIONS"
            ]
        },
        {
            "src": "/api/headEndpoint",
            "dest": "server.js",
            "methods": [
                "HEAD"
            ]
        },
        {
            "src": "/api/traceEndpoint",
            "dest": "server.js",
            "methods": [
                "TRACE"
            ]
        },
        {
            "src": "/api/connectEndpoint",
            "dest": "server.js",
            "methods": [
                "CONNECT"
            ]
        }
    ]
}